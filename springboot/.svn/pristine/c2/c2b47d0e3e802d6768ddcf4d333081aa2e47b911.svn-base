<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
							<input id="id" name="id" th:value="${bottleMaterial.id}"
								type="hidden">
							<div class="form-group">
								<label class="col-sm-3 control-label">材质名称：</label>
								<div class="col-sm-8">
									<input id="name" name="name" th:value="${bottleMaterial.name}"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">备注：</label>
								<div class="col-sm-8">
									<textarea rows="8" cols="30" name="remarks" th:text="${bottleMaterial.remarks}"></textarea>
								</div>
							</div>
							<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript"
		th:src="@{/js/appjs/basicInfo/bottleMaterial/edit.js}">
		
	</script>
</body>
</html>
