<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
																															<div class="form-group">	
								<label class="col-sm-3 control-label">二维码编号：</label>
								<div class="col-sm-8">
									<input id="qrcodeNo" name="qrcodeNo" class="form-control" type="text">
								</div>
							</div>
																									<div class="form-group">	
								<label class="col-sm-3 control-label">归属气瓶id：</label>
								<div class="col-sm-8">
									<input id="bottleId" name="bottleId" class="form-control" type="text">
								</div>
							</div>
																									<div class="form-group">	
								<label class="col-sm-3 control-label">状态（1：正常；2：报废）：</label>
								<div class="col-sm-8">
									<input id="status" name="status" class="form-control" type="text">
								</div>
							</div>
																									<div class="form-group">	
								<label class="col-sm-3 control-label">报废原因：</label>
								<div class="col-sm-8">
									<input id="scrapReason" name="scrapReason" class="form-control" type="text">
								</div>
							</div>
																									<div class="form-group">	
								<label class="col-sm-3 control-label">报废日期：</label>
								<div class="col-sm-8">
									<input id="scrapDate" name="scrapDate" class="form-control" type="text">
								</div>
							</div>
																																																																																											<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" th:src="@{/js/appjs/qrc/qrcode/add.js}">
	</script>
</body>
</html>
