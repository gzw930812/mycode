	<script th:src="@{/js/date-util.js}"></script>
	<script type="text/javascript" th:src="@{/js/plugins/layer/laydate/laydate.js}" ></script>
	<script type="text/javascript">
		function viewPic(pic_url) {
			var img = "<img src='" + fileServer + "/"+pic_url+"' onerror='defaultImg(this)' width='700px' height='500px'/>";  
			layer.open({  
			    type: 1,  
			    shade: false,  
			    title: false, //不显示标题  
			    area: [img.width + 'px', img.height+'px'],  
			    content: img, //捕获的元素，注意：最好该指定的元素要存放在body最外层，否则可能被其它的相对元素所影响  
			    cancel: function () {  
			        //layer.msg('图片查看结束！', { time: 5000, icon: 6 });  
			    }  
			});
		}
		function defaultImg(obj){
			$(obj).attr("src",ctx + "/img/404.jpg");
		}
	</script>
</div>
