<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
							<input id="id" name="id" th:value="${bottleFactory.id}"
								type="hidden">
							<div class="form-group">
								<label class="col-sm-3 control-label">公司全称：</label>
								<div class="col-sm-8">
									<input id="name" name="name" th:value="${bottleFactory.name}"
										class="form-control" type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">公司简称：</label>
								<div class="col-sm-8">
									<input id="shortName" name="shortName"
										th:value="${bottleFactory.shortName}" class="form-control"
										type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">单位代码：</label>
								<div class="col-sm-8">
									<input id="code" name="code" class="form-control"
										type="text">
								</div>
							</div>
							<div class="form-group">
								<label class="col-sm-3 control-label">气瓶制造许可证编号：</label>
								<div class="col-sm-8">
									<input id="licenseNo" name="licenseNo"
										th:value="${bottleFactory.licenseNo}" class="form-control"
										type="text">
								</div>
							</div>
							<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript"
		th:src="@{/js/appjs/basicInfo/bottleFactory/edit.js}">
		
	</script>
</body>
</html>
